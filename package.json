{
  "name": "XinYmins",
  "version": "0.12.0",
  "description": "Time tracking software to organise meetings",
  "main": "index.js",
  "scripts": {
    "serve": "parcel ./src/index.html",
    "build": "npm-run-all build:*",
    "build:cleanDist": "trash ./dist",
    "build:parcel": "parcel build ./src/index.html --public-url . --no-source-maps",
    "build:gulp": "gulp",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "docker": "docker run --rm -it -p 80:80 (docker build -q .)",
    "deploy": "npm-run-all deploy:*",
    "deploy:bump": "npm version minor",
    "deploy:build": "npm run build",
    "deploy:deploy": "gh-pages -d dist --dotfiles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Glitch0011/XinYmins.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Glitch0011/XinYmins/issues"
  },
  "homepage": "https://github.com/Glitch0011/XinYmins#readme",
  "dependencies": {
    "@createjs/tweenjs": "^2.0.0-beta.4",
    "bootstrap-vue": "^2.0.0-rc.11",
    "moment": "^2.22.2",
    "randomcolor": "^0.5.3",
    "svg.js": "^2.6.6",
    "svg2png": "^4.1.1",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-options": "^4.0.2",
    "@storybook/vue": "^4.0.2",
    "@vue/component-compiler-utils": "^2.3.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "cssnano": "^4.1.8",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-logger": "0.0.2",
    "gulp-raster": "^0.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-responsive": "^2.11.0",
    "gulp-svg2png": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "parcel-plugin-sw-precache": "^1.0.3",
    "sass": "^1.15.2",
    "trash-cli": "^1.4.0",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "workbox-build": "^3.6.3"
  }
