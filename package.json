  },
  "homepage": "https://github.com/Glitch0011/XinYmins#readme",
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.11",
    "moment": "^2.22.2",
    "randomcolor": "^0.5.3",
    "svg.js": "^2.6.6",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-options": "^4.0.2",
    "@storybook/vue": "^4.0.2",
    "@vue/component-compiler-utils": "^2.3.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "cssnano": "^4.1.8",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.15.2",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1"
